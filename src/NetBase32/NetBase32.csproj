<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <Version>0.1.3</Version>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <PropertyGroup>
  <Authors>b-higginbotham</Authors>
  <PackageTags>base32;zbase32</PackageTags>
  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  <Description>.NET Implementation of z-base-32 encoding.</Description>
  <RepositoryUrl>https://github.com/b-higginbotham/NetBase32</RepositoryUrl>
  </PropertyGroup>
</Project>
